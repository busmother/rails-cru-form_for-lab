<h1>New Song</h1>
<%= form_for(@song) do |f| %>
    <div class="field">
        <%= f.label :name, 'Name:' %><br>
        <%= f.text_field :name%>
        <%= f.label :artist_id, 'Artist ID:' %><br>
        <%= f.text_field :artist_id%>
        <%= f.label :genre_id, 'Genre ID:' %><br>
        <%= f.text_field :genre_id%>
    </div>

    <div class="actions">
        <%= f.submit%>
    </div>
<%end%>