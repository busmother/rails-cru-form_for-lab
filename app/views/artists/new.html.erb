<h1>New Artist</h1>
<%= form_for(@artist) do |f| %>
    <div class="field">
        <%= f.label :name, 'Name:' %><br>
        <%= f.text_field :name%>
        <%= f.label :bio, 'Bio:' %><br>
        <%= f.text_field :bio%>
    </div>

    <div class="actions">
        <%= f.submit%>
    </div>
<%end%>